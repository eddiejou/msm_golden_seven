<div  class="page-header">
  <h1>
    Actor #<%= @actor.id %>
  </h1>
</div>



<dl class="dl-horizontal">
  <dt>DOB</dt>
  <dd><%= @actor.dob %></dd>

  <dt>Name</dt>
  <dd><%= @actor.name %></dd>

  <dt>Bio</dt>
  <dd><%= @actor.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @actor.image_url %>"></dd>

  <dd><a href="/actors" class="btn btn-primary">Go Back</a></dd>

</dl>
